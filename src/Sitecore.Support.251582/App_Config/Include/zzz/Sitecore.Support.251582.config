      <httpRequestProcessed>
        <processor patch:instead="*[@type='Sitecore.XA.Feature.ErrorHandling.Pipelines.HtpRequestProcessed.HandleServerErrorCode, Sitecore.XA.Feature.ErrorHandling']" type="Sitecore.Support.XA.Feature.ErrorHandling.Pipelines.HtpRequestProcessed.HandleServerErrorCode, Sitecore.Support.251582" resolve="true" />
      </httpRequestProcessed>
    </pipelines>
    <services>
      <register patch:instead="*[@serviceType='Sitecore.XA.Foundation.Multisite.ISiteInfoResolver, Sitecore.XA.Foundation.Multisite']"
  serviceType="Sitecore.XA.Foundation.Multisite.ISiteInfoResolver, Sitecore.XA.Foundation.Multisite" implementationType="Sitecore.Support.XA.Foundation.Multisite.SiteInfoResolver, Sitecore.Support.251582" lifetime="Singleton"/>
    </services>
  </sitecore>
</configuration>